<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @person.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @person.last_name %>
</p>

<p>
	<strong>Phone Numbers:</strong>
	<% @person.phone_numbers.each do |phone| %>
		<li><%= phone.number %> </li>
	<% end %>
</p>

<%= link_to 'Edit', edit_person_path(@person) %> 
<%= link_to 'Back', people_path %>
